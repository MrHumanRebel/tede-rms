                            [
                                {
                                    text: "Tartalmazza az összeépítést, szétszerelést illetve a különböző rendszerek működtetését.",
                                    style: { fontSize: '{{ newFontSize }}' },
                                    margin: [0, 0, 0, 5]  
                                }
                            ],
                            [
                                {
                                    text: "Tartalmazza a rendszerek teljeskörű működéséhez szükséges eszközpark és személyzet szállítását.",
                                    style: { fontSize: '{{ newFontSize }}' },
                                    margin: [0, 0, 0, 5]  
                                }
                            ]
                        ]
                    },
                    fillColor: '#F5F5F5',
                    layout: 'noBorders',
                    margin: [0, 0, 0, 0]
                },
                {% endif %}
                {% if GET['paymentTerms'] %}
                {
                    "table": {
                        "widths": ["50%", "50%"],
                        "body": [
                            [
                                {
                                    text: "Dátum: {{ 'now' | date('Y. m. d.') | escape('js') }}", 
                                    alignment: "center",
                                    border: [false, false, false, false],
                                    margin: [0, 0, 0, 60]
                                },
                                { 
                                    text: "Az ajánlatot elfogadom",
                                    alignment: "center",
                                    border: [false, true, false, false],
                                    margin: [50, 0, 50, 0], // Left, Top, Right, Bottom
                                    borderWidth: 10
                                },
                            ]
                        ]
                    },
                    margin: [0, 60, 40, 0] // Left, Top, Right, Bottom
                },
                {% endif %}

            ],
            header: function(currentPage) { 
                return {
                    stack: [
                        {% if INSTANCELOGO %}
                        {
                            image: "{{ INSTANCELOGO|escape('js') }}",
                            width: 515,
                            alignment: 'center',
                            margin: [0, 0, 0, 0]

                        },
                        {% endif %}
                    ],
                    margin: [0, 20, 0, 0] // Left, Top, Right, Bottom
                };           
            },
            footer: function(currentPage, pageCount) { 
                return {
                    stack: [
                        // Line with margin below it
                        { 
                            canvas: [ 
                                { 
                                    type: 'line', 
                                    x1: 0, 
                                    y1: 0, 
                                    x2: 510, 
                                    y2: 0, 
                                    lineWidth: 1 
                                } 
                            ],
                            alignment: 'center',
                            margin: [0, 0, 0, 5] 
                        },
                        // Table with text and page count
                        {
                            table: {
                                widths: ['100%'],
                                body: [
                                    [
                                        { 
                                            text: "Tamás Dávid EV.\t\t\t     Cím: 9174 Dunaszeg, Iskola utca 7.\t\t\t    Telefon: +36 30 242 55 44 \n" +
                                                "Bankszámla szám: 11737007-20749635\t\t  Adószám: 60721561-2-28\t\t  Mail: info@tede.hu\n" + 
                                                currentPage.toString() + " / " + pageCount.toString(), 
                                            alignment: 'center' 
                                        }
                                    ],
                                ]
                            },
                            layout: 'noBorders'
                        }
                    ],
                    margin: [0, 0, 0, 0]  // Left, Top, Right, Bottom
                }; 
            },

            {% if GET['draft'] %}watermark: { text: 'MINTA', opacity: 0.2, bold: true, italics: false },{% endif %}
            pageSize: 'A4',
            pageOrientation: 'portrait',
            pageMargins: [ 40, 100, 40, 90 ],
            info: {
                title: "{% if project.projectsTypes_config_client == 1 and project['clients_name'] %}{{  project['clients_name']|escape("js") }} - {% endif %}{{ project['projects_name']|escape("js") }}",
                author: "{{ USERDATA['instance']['instances_name']|escape("js") }}",
                subject: "{{ project['projects_name']|escape("js") }}",
                keywords: 'TeDeRMS',
            },
            compress: true,
        };
        try {
            var pdf = pdfMake.createPdf(docDefinition);
            {% if GET['draft'] or CONFIG.FILES_ENABLED != "Enabled" or USERDATA.instance.instances_storageEnabled != 1 %}
                pdf.download("{{ project['projects_name']|escape("js") }} - Draft {{ GET['quote'] == 'true' ? 'Quote' : (GET['quote'] == 'truck' ? 'Truck' : 'Invoice') }}.pdf", function() {
                    setTimeout(() => {window.close()}, 100);
                });
            {% else %}
                pdf.getBlob((blob) => {
                    var formData = new FormData();
                    formData.append("file", blob, {type: "application/pdf"});
                    formData.append("quote", "{{ GET['quote'] }}");
                    formData.append("fileNumber", "{{ fileNumber|escape('js') }}");
                    formData.append("id", "{{ project['projects_id'] }}");

                    // Log all formData key-value pairs
                    for (var pair of formData.entries()) {
                        console.log(pair[0] + ": " + pair[1]);
                    }
                    var oReq = new XMLHttpRequest();
                    oReq.open("POST", "{{CONFIG.ROOTURL}}/api/s3files/uploadProjectInvoice.php", true);
                    oReq.onload = function (oEvent) {
                        var response = JSON.parse(oReq.responseText);
                        console.log(response);
                        if (response.error && response.error.code && response.error.code == "AUTH") {
                            location.reload();
                        } else if (response.error && response.error.code) {
                            alert(response.error.message);
                        } else if (response.result && response.response && response.response.url) {
                            pdf.download("{{ project['projects_name']|escape("js") }} - {{ GET['quote'] == 'true' ? 'Quote' : (GET['quote'] == 'truck' ? 'Truck' : 'Invoice') }} v{{ fileNumber|escape("js") }}.pdf", function() {
                                setTimeout(() => {window.close()}, 100);
                            });
                        } else {
                            alert("Error loading PDF");
                        }
                    };
                    oReq.onerror = function(error) {
                        alert("Error creating file");
                    };
                    oReq.send(formData);
                });
            {% endif %}
        } catch(err) {
            alert(err.message);
        }

    </script>
